{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs",
    "outDir": "./dist",
    "sourceMap": true,
    "strict": true,
    "target": "es6",
    "types": ["node", "jest"]
  },
  "include": ["lib_core", "lib_mat", "src", "bin"],
  "exclude": ["node_modules/*"]
}
